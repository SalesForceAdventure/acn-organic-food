<?xml version="1.0" encoding="UTF-8"?>
<shipping xmlns="http://www.demandware.com/xml/impex/shipping/2007-03-31">
    <shipping-method method-id="Padrão" default="true">
        <display-name xml:lang="x-default">Padrão</display-name>
        <online-flag>true</online-flag>
        <tax-class-id>standard</tax-class-id>
        <price-table>
            <amount order-value="10">10</amount>
        </price-table>
        <custom-attributes>
            <custom-attribute attribute-id="estimatedArrivalTime" xml:lang="x-default">Dois dias uteis</custom-attribute>
            <custom-attribute attribute-id="storePickupEnabled">false</custom-attribute>
        </custom-attributes>
    </shipping-method>

    <shipping-method method-id="Retira" default="false">
        <display-name xml:lang="x-default">Retira</display-name>
        <online-flag>true</online-flag>
        <tax-class-id>standard</tax-class-id>
        <price-table>
            <amount order-value="0.0">0.0</amount>
        </price-table>
        <custom-attributes>
            <custom-attribute attribute-id="estimatedArrivalTime" xml:lang="x-default">Um dia útil</custom-attribute>
            <custom-attribute attribute-id="storePickupEnabled">true</custom-attribute>
        </custom-attributes>
    </shipping-method>

</shipping>
